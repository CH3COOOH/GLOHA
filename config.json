{
	"RED_TEST_1":
	{
		"check_interval": 10,
		"check_scheme": "tcp",
		"run_mode": "redirect:tcp:192.168.10.185:80",
		"server_list":{
			"0":{
				"host": "127.0.0.1:8000",
				"target": "192.168.10.1:8800",
				"timeout": 200
			},
			"1":{
				"host": "127.0.0.1:8001",
				"target": "192.168.10.2:8001",
				"timeout": 200
			}
		}
	},
	"RED_TEST_2":
	{
		"check_interval": 10,
		"check_scheme": "icmp",
		"run_mode": "redirect:tcp:192.168.10.185:81",
		"server_list":{
			"0":{
				"host": "127.0.0.1",
				"target": "192.168.10.2:8800",
				"timeout": 200
			},
			"1":{
				"host": "127.0.0.1",
				"target": "192.168.10.2:8001",
				"timeout": 200
			}
		}
	},
	"LINE_WORLD":
	{
		"check_interval": 20,
		"check_scheme": "icmp",
		"run_mode": "ps",
		"server_list":
		{
			"0":
			{
				"host": "127.0.0.1",
				"exec": "gost -L=socks://127.0.0.1:1080",
				"timeout": 200
			},
			"1":
			{
				"host": "localhost",
				"exec": "gost -L=socks://127.0.0.1:1081",
				"timeout": 300
			}
		}
	},

	"LINE_LOCAL":
	{
		"check_interval": 15,
		"check_scheme": "tcp",
		"run_mode": "ps",
		"server_list":
		{
			"0":
			{
				"host": "localhost:8000",
				"exec": "gost -L=http://127.0.0.1:8080",
				"timeout": 200
			},
			"1":
			{
				"host": "localhost:8001",
				"exec": "gost -L=http://127.0.0.1:8082",
				"timeout": 300
			}
		}
	}
}
